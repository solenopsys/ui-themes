{
  "name": "themes",
  "projectType": "library",
  "sourceRoot": "packages/uimatrix/themes/src",
  "prefix": "sc",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/uimatrix/themes"
      ],
      "options": {
        "jestConfig": "packages/uimatrix/themes/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/uimatrix/themes/src/**/*.ts",
          "packages/uimatrix/themes/src/**/*.html"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "tags": []
}
